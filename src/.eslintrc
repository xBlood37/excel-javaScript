{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "linebreak-style": 0,
    "quotes": "off",
    "object-curly-spacing": "warn",
    "operator-linebreak": "off",
    "no-debugger": "off",
    "no-unused-vars": "warn",
    "indent": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
